          <Component Id="ShortcutsComponent" Guid="c13e9961-6a30-43f5-8363-4091073bc188">
            <Shortcut Id="GameShortcut" Directory="GameProgramMenuFolder" Name="The Legend of Zelda - Black Crown" Description="Click to start the game! \o/" Target="[INSTALLDIR]TLoZ - Black Crown.exe" WorkingDirectory="INSTALLDIR" />

